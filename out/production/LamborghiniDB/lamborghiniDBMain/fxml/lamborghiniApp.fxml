<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane id="A2" prefHeight="500.0" prefWidth="600.0" stylesheets="@aaa.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lamborghiniDBMain.LoadingController">
   <children>
      <VBox alignment="CENTER" prefHeight="500.0" prefWidth="600.0">
         <children>
            <VBox id="A4" alignment="CENTER" prefHeight="114.0" prefWidth="600.0" stylesheets="@aaa.css">
               <children>
                  <Label text="Enter to programm as" textFill="#6b5f0e">
                     <font>
                        <Font name="tahoma" size="22.0" />
                     </font>
                  </Label>
                  <HBox alignment="CENTER" spacing="50.0">
                     <children>
                        <Button id="A1" mnemonicParsing="false" onAction="#employeeEnter" prefWidth="150.0" stylesheets="@aaa.css" text="Employee" />
                        <Button id="A1" mnemonicParsing="false" onAction="#userEnter" prefWidth="150.0" stylesheets="@aaa.css" text="Client" />
                        <Button id="A1" mnemonicParsing="false" onAction="#adminEnter" prefWidth="150.0" stylesheets="@aaa.css" text="Administrator" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
               </children>
            </VBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button id="A3" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#exitProgram" prefWidth="150.0" stylesheets="@aaa.css" text="Quit" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane id="A2" prefHeight="500.0" prefWidth="600.0" stylesheets="@aaa.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lamborghiniDBMain.UserModeController">
   <children>
      <VBox alignment="CENTER" prefHeight="500.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Leave an information about yourself" />
            <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="0.0" spacing="10.0">
               <children>
                  <Label text="Card Number" />
                  <TextField fx:id="cardNumber">
                     <HBox.margin>
                        <Insets right="200.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="39.0" prefWidth="31.0" spacing="10.0">
               <children>
                  <Label text="Name" />
                  <TextField fx:id="name">
                     <HBox.margin>
                        <Insets right="200.0" />
                     </HBox.margin></TextField>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="0.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label text="Phone Number" />
                  <TextField fx:id="phoneNumber">
                     <HBox.margin>
                        <Insets right="200.0" />
                     </HBox.margin></TextField>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Button id="A1" mnemonicParsing="false" onAction="#orderCar" stylesheets="@aaa.css" text="Order" />
                  <Button id="A1" mnemonicParsing="false" onAction="#toPersonnelOrder" stylesheets="@aaa.css" text="Back" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</Pane>
